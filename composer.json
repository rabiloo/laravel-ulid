{
  "name": "rabiloo/laravel-ulid",
  "description": "This is my package laravel-ulid",
  "keywords": [
    "rabiloo",
    "laravel-ulid"
  ],
  "homepage": "https://github.com/rabiloo/laravel-ulid",
  "license": "MIT",
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "vimeo/psalm": "^4.11"
  },
  "autoload": {
    "psr-4": {
      "Rabiloo\\Laravel\\Ulid\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rabiloo\\Laravel\\Ulid\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/psalm",
    "format": "vendor/bin/phpcbf && vendor/bin/pint",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
